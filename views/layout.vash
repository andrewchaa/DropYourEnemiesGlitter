<!DOCTYPE html>
<html lang="en">
<head>@html.include("head")</head>
 
<body>

<script>
  window.fbAsyncInit = function() {
    FB.init({
      appId      : '801179666614539',
      xfbml      : true,
      version    : 'v2.2'
    });
  };

  (function(d, s, id){
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) {return;}
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));
</script>

<!-- begin preloader -->
<div class="preloader">
    <div class="preloader-content-wrapper">
        <div class="preloader-content">
            <i class="fa fa-cog fa-3x fa-spin"></i>
        </div>
    </div>
</div>
<!-- end preloader -->

@html.block("main")

<!-- begin footer section -->
<footer class="footer" id="footer">
	<div class="container">
		<div class="row wow fadeIn animated" data-wow-offset="10" data-wow-duration="1.5s">
			<div class="col-md-12 text-center">
        <div class="fb-like" data-href="https://developers.facebook.com/docs/plugins/" data-layout="button_count" data-action="like" data-show-faces="true" data-share="true"></div>
				<p><small>Copyright &copy; 2015 Drop your enemies glitter</small></p>
			</div>
		</div>

	</div>
</footer>
<!-- end footer section -->

<!-- Javascripts -->
<script src="/js/jquery.js"></script>
<script src="/js/bootstrap.min.js"></script>
<script src="/js/owl.carousel.min.js"></script>
<script src="/js/jquery.fitvids.js"></script>
<script src="/js/smoothScroll.js"></script>
<script src="/js/nivo-lightbox.min.js"></script>
<script src="/js/jquery.scrollTo.js"></script>
<script src="/js/jquery.nav.js"></script>
<script src="/js/wow.min.js"></script>
<script src="/js/jquery.jigowatt.js"></script>

<!-- Custom Javascript -->
<script src="/js/main.js"></script>

